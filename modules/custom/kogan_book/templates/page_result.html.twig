  <defs>
    <marker id="arrow" markerWidth="10" markerHeight="10" refX="2" refY="3" orient="auto" markerUnits="strokeWidth">
      <path d="M0,0 L0,6 L9,3 z" fill="#f00" />
    </marker>
  </defs>

  <line x1="{{ arrowx1 }}" y1="{{ arrowy1 }}" x2="{{ arrowx2 }}" y2="{{ arrowy2 }}" stroke="#000" stroke-width="5" marker-end="url(#arrow)" />
{# 
  <text x="{{ frontx }}" y="{{ fronty }}" dy=".35em">Front end</text>
