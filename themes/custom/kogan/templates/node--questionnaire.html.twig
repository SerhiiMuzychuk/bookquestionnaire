<div class="container">
  <div class="contact-data">
    <p class="contacts">Personal info</p>
    <div class="contact-name">
      <label>Name:</label>
      {{ name }}
    </div>
    <div class="contact-company">
      <label>Company:</label>
      {{ company }}
    </div>
    <div class="contact-company">
      <label>Industry:</label>
      {{ industry }}
    </div>
    <div class="contact-company-size">
      <label>Company size:</label>
      {{ company_size }}
    </div>
    <div class="contact-title">
      <label>E-mail:</label>
      {{ title }}
    </div>
    <div class="contact-created">
      <p class="lable-created">The test was completed:</p>{{ created |date("d.m.Y - g:i A")}}
    </div>  
  </div>
</div>

<style type="text/css">

.graph .labels.x-labels {
  text-anchor: middle;
}

.graph .labels.y-labels {
  text-anchor: end;
}


.graph {
  height: 500px;
  width: 500px;
  background-color: black !important;
}

text {
  fill: white;
  font-size: 20%;
}

.graph .grid {
  stroke-dasharray: 0;
  stroke-width: 1;
}

.labels {
  font-size: 100%;
}

.label-title {
  font-weight: bold;
  text-transform: uppercase;
  font-size: 70;
  fill: white;
}

.data {
  fill: #ffbe18;
  stroke-width: 1; 
}

@media screen and (max-width: 1599px) and (min-width: 1100px)  {
  .graph {
    height: 350px;
    width: 350px;
    margin-top: 20px;
  }
}

@media screen and (max-width: 479px) {
  .graph {
    height: 210px;
    width: 210px;
    margin-top: 20px;
  }
}

</style>
